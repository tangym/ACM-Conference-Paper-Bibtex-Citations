 isbn = {978-1-4503-4675-7},
 keyword = {approximation algorithm, combinatorial optimization, dataset summarization, snippet generation},
 link = {http://doi.acm.org/10.1145/3018661.3018670},
 location = {Cambridge, United Kingdom},
 numpages = {9},
 pages = {151--159},
 publisher = {ACM},
 series = {WSDM '17},
 title = {Generating Illustrative Snippets for Open Data on the Web},
 year = {2017}
}


