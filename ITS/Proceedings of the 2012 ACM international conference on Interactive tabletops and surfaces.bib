 booktitle = {Proceedings of the 2012 ACM International Conference on Interactive Tabletops and Surfaces},
 doi = {10.1145/2396636.2396639},
 isbn = {978-1-4503-1209-7},
 keyword = {3d interactions, 3d tabletops, co-planar dragging, direct manipulation, motion parallax},
 link = {http://doi.acm.org/10.1145/2396636.2396639},
