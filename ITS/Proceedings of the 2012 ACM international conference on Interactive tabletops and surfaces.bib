 author = {Marquardt, Nicolai and Ballendat, Till and Boring, Sebastian and Greenberg, Saul and Hinckley, Ken},
 booktitle = {Proceedings of the 2012 ACM International Conference on Interactive Tabletops and Surfaces},
 doi = {10.1145/2396636.2396642},
 isbn = {978-1-4503-1209-7},
 keyword = {awareness, gradual engagement, handhelds, interactive surfaces, proxemic interactions, proximity},
 link = {http://doi.acm.org/10.1145/2396636.2396642},
 location = {Cambridge, Massachusetts, USA},
 numpages = {10},
 pages = {31--40},
 publisher = {ACM},
 series = {ITS '12},
 title = {Gradual Engagement: Facilitating Information Exchange Between Digital Devices As a Function of Proximity},
 year = {2012}
}


