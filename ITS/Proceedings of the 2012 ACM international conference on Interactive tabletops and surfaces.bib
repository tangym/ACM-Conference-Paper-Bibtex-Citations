@proceedings{Quigley:2013:2512349,
 abstract = {Welcome to the ACM International Conference on Interactive Tabletops and Surfaces (ITS 2013) in St Andrews, Scotland. This year ITS received a total of 121 valid submissions including 86 papers and 35 notes; this represents an increase of 16% over last year. Of these submissions, 27 papers and 8 notes were accepted, resulting in an overall acceptance rate of 29%--close to last year's, but with an increase in the number of accepted submissions (5 more than last year, or a 17% increase). The number and quality of this year's submissions cement ITS's reputation as the leading venue for work on interactive surfaces and highlight the increasing relevance of this area within Human-Computer Interaction. In response to feedback from the community, this is the first time that the conference has included application papers as a distinct type of paper or note. The response has been outstanding. We received an abundance of high quality application paper submissions from industry and academia that bridge research and practice and provide valuable insights from real-world deployments. Application papers accepted this year are part of the regular program and are distinguishable by an indication on the first page. The peer review process for both application and academic papers was rigorous: all papers received at least two reviews by external experts and one review by a member of the committee. A large number of papers received also additional reviews, meta-reviews, and full reviews by a second member of the committee. The application paper review process differed from the academic review process in two respects. First, application reviews were guided by a set of five questions that brought attention to the domain of the application, the design process, the integration of multiple existing technologies, and practical issues derived from the implementation and deployment of surfaces in a real-world context. Second, application papers received more reviews from members of the ITS community outside of academia.},
 address = {New York, NY, USA},
 isbn = {978-1-4503-2271-3},
 location = {St. Andrews, Scotland, United Kingdom},
 note = {505132},
 publisher = {ACM},
 title = {ITS '13: Proceedings of the 2013 ACM International Conference on Interactive Tabletops and Surfaces},
 year = {2013}
}


