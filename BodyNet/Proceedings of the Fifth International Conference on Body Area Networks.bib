 location = {Corfu, Greece},
 numpages = {4},
 pages = {33--36},
 publisher = {ACM},
 series = {BodyNets '10},
 title = {DynAGreen: Hierarchical Dynamic Energy Efficient Task Assignment for Wireless Healthcare Systems},
 year = {2010}
}


