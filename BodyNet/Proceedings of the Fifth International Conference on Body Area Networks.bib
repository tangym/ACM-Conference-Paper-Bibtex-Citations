 keyword = {Bluetooth, ECG, Java, MATLAB, PPG, de-noising, discrete wavelet transform, smart-phone, wavelet packets, wearable health-monitoring system},
 link = {http://doi.acm.org/10.1145/2221924.2221966},
 location = {Corfu, Greece},
 numpages = {7},
