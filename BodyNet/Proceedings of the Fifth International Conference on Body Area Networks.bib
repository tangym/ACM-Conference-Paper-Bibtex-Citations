 location = {Corfu, Greece},
 numpages = {7},
 pages = {188--194},
 publisher = {ACM},
 series = {BodyNets '10},
 title = {Monitoring Body Temperature of Newborn Infants at Neonatal Intensive Care Units Using Wearable Sensors},
 year = {2010}
}


