 keyword = {classification, clustering, feature synthesis, functional MRI, neuroscience},
 link = {http://doi.acm.org/10.1145/2020408.2020563},
 location = {San Diego, California, USA},
 numpages = {7},
 pages = {940--946},
 publisher = {ACM},
 series = {KDD '11},
 title = {Classification of Functional Magnetic Resonance Imaging Data Using Informative Pattern Features},
 year = {2011}
}


