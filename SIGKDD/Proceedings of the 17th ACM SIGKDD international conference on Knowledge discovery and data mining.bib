 isbn = {978-1-4503-0813-7},
 keyword = {data mining, disaster information management, dynamic query form, hierarchical summarization, user recommendation},
 link = {http://doi.acm.org/10.1145/2020408.2020457},
 location = {San Diego, California, USA},
 numpages = {9},
