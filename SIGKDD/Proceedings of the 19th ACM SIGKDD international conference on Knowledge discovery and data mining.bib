 numpages = {9},
 pages = {176--184},
 publisher = {ACM},
 series = {KDD '13},
 title = {Succinct Interval-splitting Tree for Scalable Similarity Search of Compound-protein Pairs with Property Constraints},
 year = {2013}
}


